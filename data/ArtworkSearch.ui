<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkImage" id="image1">
    <property name="can_focus">0</property>
    <property name="icon_name">image-x-generic-symbolic</property>
  </object>
  <object class="GtkGrid" id="widget">
    <property name="can_focus">0</property>
    <property name="orientation">vertical</property>
    <property name="row_spacing">15</property>
    <child>
      <object class="GtkBox">
        <property name="can_focus">0</property>
        <property name="margin-start">5</property>
        <property name="margin-end">5</property>
        <property name="margin_top">5</property>
        <property name="spacing">5</property>
        <child>
          <object class="GtkLabel" id="label">
            <property name="can_focus">0</property>
            <property name="halign">start</property>
          </object>
        </child>
        <child>
          <object class="GtkSpinner" id="spinner">
            <property name="can_focus">0</property>
            <property name="halign">start</property>
            <property name="hexpand">1</property>
            <property name="active">True</property>
          </object>
        </child>
        <child>
          <object class="GtkSearchEntry" id="entry">
            <property name="primary_icon_name">edit-find-symbolic</property>
            <property name="primary_icon_activatable">False</property>
            <property name="primary_icon_sensitive">False</property>
            <signal name="search-changed" handler="_on_search_changed" swapped="no"/>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="button1">
            <property name="receives_default">1</property>
            <property name="tooltip_text" translatable="yes">Select a local file</property>
            <property name="halign">end</property>
            <property name="image">image1</property>
            <property name="relief">none</property>
            <signal name="clicked" handler="_on_button_clicked" swapped="no"/>
          </object>
        </child>
        <child>
          <placeholder/>
        </child>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
