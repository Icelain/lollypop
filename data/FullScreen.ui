<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkImage" id="close_image">
    <property name="can-focus">0</property>
    <property name="icon-name">window-close-symbolic</property>
  </object>
  <object class="GtkButton" id="close_btn">
    <property name="receives-default">1</property>
    <property name="halign">end</property>
    <property name="valign">start</property>
    <property name="image">close_image</property>
    <property name="relief">none</property>
    <signal name="clicked" handler="_on_close_button_clicked" swapped="no"/>
    <style>
      <class name="fullscreen-close-button"/>
    </style>
  </object>
  <object class="GtkImage" id="pan_image">
    <property name="can-focus">0</property>
    <property name="icon-name">pan-start-symbolic</property>
  </object>
  <object class="GtkOverlay" id="widget">
    <property name="can-focus">0</property>
    <property name="child">
      <object class="GtkImage" id="background_artwork">
        <property name="can-focus">0</property>
      </object>
    </property>
    <child type="overlay">
      <object class="GtkGrid" id="overlay_grid">
        <property name="can-focus">0</property>
        <property name="can-default">True</property>
        <property name="margin-bottom">20</property>
        <property name="row-spacing">30</property>
        <property name="column-homogeneous">1</property>
        <child>
          <object class="GtkLabel" id="datetime">
            <property name="can-focus">0</property>
            <property name="valign">start</property>
            <property name="vexpand">1</property>
            <attributes>
              <attribute name="weight" value="bold"></attribute>
            </attributes>
            <style>
              <class name="dim-label"/>
              <class name="gnome-shell"/>
            </style>
            <layout>
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
      </object>
    </child>
    <child type="overlay">
      <object class="GtkGrid">
        <property name="can-focus">0</property>
        <property name="halign">end</property>
        <property name="margin-top">48</property>
        <child>
          <object class="GtkRevealer" id="revealer">
            <property name="can-focus">0</property>
            <property name="transition-type">slide-left</property>
            <child>
              <placeholder/>
            </child>
            <layout>
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkButton">
            <property name="width-request">32</property>
            <property name="height-request">32</property>
            <property name="receives-default">1</property>
            <property name="valign">end</property>
            <property name="image">pan_image</property>
            <property name="relief">none</property>
            <signal name="clicked" handler="_on_reveal_button_clicked" swapped="no"/>
            <style>
              <class name="reveal-button-fullscreen"/>
              <class name="banner-button"/>
            </style>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <style>
          <class name="transparent-grid-fullscreen"/>
        </style>
      </object>
    </child>
  </object>
</interface>
