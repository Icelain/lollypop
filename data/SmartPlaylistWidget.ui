<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAdjustment" id="adjustment1">
    <property name="lower">10</property>
    <property name="upper">3000</property>
    <property name="value">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkImage" id="image1">
    <property name="can-focus">0</property>
    <property name="icon-name">user-trash-symbolic</property>
  </object>
  <object class="GtkGrid" id="widget">
    <property name="can-focus">0</property>
    <property name="margin-top">5</property>
    <property name="margin-bottom">5</property>
    <property name="column-spacing">5</property>
    <child>
      <object class="GtkComboBoxText" id="type_combobox">
        <property name="can-focus">0</property>
        <items>
          <item id="genre" translatable="yes">Genre</item>
          <item id="artist" translatable="yes">Artist</item>
          <item id="album" translatable="yes">Album</item>
          <item id="rating" translatable="yes">Rating</item>
          <item id="popularity" translatable="yes">Popularity</item>
          <item id="year" translatable="yes">Year</item>
          <item id="bpm" translatable="yes">BPM</item>
        </items>
        <signal name="changed" handler="_on_type_combobox_changed" swapped="no"/>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkComboBoxText" id="operand_combobox">
        <property name="can-focus">0</property>
        <layout>
          <property name="column">1</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkStack" id="stack">
        <property name="can-focus">0</property>
        <property name="hexpand">1</property>
        <property name="transition-type">crossfade</property>
        <child>
          <object class="GtkStackPage">
            <property name="name">text</property>
            <property name="child">
              <object class="GtkEntry" id="entry"/>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkStackPage">
            <property name="name">rating</property>
            <property name="position">1</property>
            <property name="child">
              <object class="GtkGrid" id="rating_widget">
                <property name="can-focus">0</property>
                <property name="halign">center</property>
                <property name="valign">center</property>
                <child>
                  <object class="GtkImage" id="star0">
                    <property name="can-focus">0</property>
                    <property name="opacity">0.20</property>
                    <property name="icon-name">starred-symbolic</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkImage" id="empty_star1">
                    <property name="can-focus">0</property>
                    <property name="opacity">0</property>
                    <property name="icon-name">starred-symbolic</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkImage" id="star1">
                    <property name="can-focus">0</property>
                    <property name="opacity">0.20</property>
                    <property name="icon-name">starred-symbolic</property>
                    <layout>
                      <property name="column">2</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkImage" id="star2">
                    <property name="can-focus">0</property>
                    <property name="opacity">0.20</property>
                    <property name="icon-name">starred-symbolic</property>
                    <layout>
                      <property name="column">3</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkImage" id="star3">
                    <property name="can-focus">0</property>
                    <property name="opacity">0.20</property>
                    <property name="icon-name">starred-symbolic</property>
                    <layout>
                      <property name="column">4</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkImage" id="star4">
                    <property name="can-focus">0</property>
                    <property name="opacity">0.20</property>
                    <property name="icon-name">starred-symbolic</property>
                    <layout>
                      <property name="column">5</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkStackPage">
            <property name="name">int</property>
            <property name="position">2</property>
            <property name="child">
              <object class="GtkSpinButton" id="spin_button">
                <property name="adjustment">adjustment1</property>
              </object>
            </property>
          </object>
        </child>
        <layout>
          <property name="column">2</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkButton">
        <property name="receives-default">1</property>
        <property name="image">image1</property>
        <signal name="clicked" handler="_on_destroy_button_clicked" swapped="no"/>
        <style>
          <class name="menu-button"/>
        </style>
        <layout>
          <property name="column">3</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
  </object>
</interface>
